  "main": "index.js",
  "author": "Lucas William <lucaswilliameufrasio@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "dev": "yarn dev:base --open",
    "build": "webpack --config webpack.prod.js",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev:base http-get://localhost:8080 test:cypress:run",
    "check": "npm-check -u"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/axios": "^0.14.0",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cypress": "^5.3.0",
    "eslint": "^7.19.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^5.2.0",
    "favicons-webpack-plugin": "^4.2.0",
    "full-icu": "^1.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.6",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "start-server-and-test": "^1.12.0",
    "style-loader": "^2.0.0",
